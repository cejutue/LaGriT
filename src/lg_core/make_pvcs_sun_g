 C_CODE = matrix_values_compress \
 anothermatbld3d \
 sparseMatrix
build_lagrit : lagrit.a
%ForEach object in ${C_CODE}
lagrit.a : $(object).o
	ar rcu lagrit.a $(_NewSources)
%EndFor
%ForEach file in ${C_CODE}
$(file).o : $(file).c $(INCLUDE_CH)
	${CC} ${CFLAGS} $(_Source) -c
%EndFor
build_lagrit : lagrit.a
%ForEach object in ${FORTRAN90_CODE}
lagrit.a : $(object).o
	ar rcu lagrit.a $(_NewSources)
%EndFor
%ForEach file in ${FORTRAN90_CODE}
$(file).o : $(file).f90 
	${FC90} ${FF90FLAGS} $(_Source) -c
%EndFor
%ForEach object in ${FORTRAN_CODE}
lagrit.a : $(object).o
	ar rcu lagrit.a $(_NewSources)
%EndFor
%ForEach file in ${FORTRAN_CODE}
$(file).o : $(file).f $(INCLUDE_X3D)
	${FC} ${FFLAGS} $(_Source) -c
%EndFor
